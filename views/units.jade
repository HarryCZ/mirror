extends layoutblock subnav	ul		li <a href="/units">Units</a>block content	h1=title	table.usertable		thead			tr				th.					Název				th.					Značka				th.					Podjednotka				th.					Značka podjednotky				th.					Přepočet				th.					Deleted		tbody			each unit, i in units				tr					td.						#{unit.title}					td.						#{unit.sign}					td.						#{unit.subunit}					td.						#{unit.subunit_sign}					td.						#{unit.conversion}					td.						#{unit.deleted}							p.		<br>		<a href="/storage">Go Back</a>block sidebar	.block		h1 Add Unit		form.addUser(name="addunit",method="post",action="/addunit")			input(type="text", placeholder="Název", name="title")			input(type="text", placeholder="Značka", name="sign")			input(type="text", placeholder="Podjednotka", name="subunit")			input(type="text", placeholder="Značka podjednotky", name="subunit_sign")			input(type="text", placeholder="Přepočet", name="conversion")			input(type="text", placeholder="deleted", name="deleted")			button#btnSubmit(type="submit") submit