extends layoutblock scripts	script.		$(document).ready(function() {			alignTable();		});block subnav	#subnavBtn		mtl.			units	.subnav		ul			a(href="/storage")				li					mtl.						storage			a(href="/units")				li					mtl.						units	#addBtn		a(href="/addUnit")			img(src="/images/butAdd.png")			block content	h1		mtl.			units	table.unitstable		thead			tr				th					mtl.						title				th.odd					mtl.						sign		tbody			each unit, i in units				tr					td.column-0.						#{unit.title}					td(class="column-1 odd").						#{unit.sign}					td						form.remUnit(name="remUnit",method="post",action="/remUnit", onsubmit="return confirm('Are you sure you want to submit?')")							input(type="hidden", name="remId" value="#{unit._id}")							button#btnSubmit(type="submit") X					td.butEdit						a(href="/unitEdit#{unit._id}")							mtl.								edit