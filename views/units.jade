extends layoutblock subnav	ul		li <a href="/units">Units</a>block content	h1=title	table.usertable		thead			tr				th.					Title				th.					Sign				th.										Subunit				th.					Subunit sign				th.					Conversion				th.					Deleted		tbody			each unit, i in units				tr					td.						#{unit.title}					td.						#{unit.sign}					td.						#{unit.subunit}					td.						#{unit.subunit_sign}					td.						#{unit.conversion}					td.						#{unit.deleted}							p.		<br>		<a href="/storage">Go Back</a>block sidebar	.block		h1 Add Unit		form.addUser(name="addunit",method="post",action="/addunit")			input(type="text", placeholder="Title", name="title")			input(type="text", placeholder="Sign", name="sign")			input(type="text", placeholder="Subunit", name="subunit")			input(type="text", placeholder="Subunit sign", name="subunit_sign")			input(type="text", placeholder="Conversion", name="conversion")			input(type="text", placeholder="deleted", name="deleted")			button#btnSubmit(type="submit") submit