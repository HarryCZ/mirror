extend layoutblock scripts	script.		treasuriesArr = "#{treasuriesArr}";		incomesSpendingsArr = "#{incomesSpendingsArr}";	script(type='text/javascript' src='/js/incomesSpending.js')block subnav	#subnavBtn		mtl.			incomes_spending	.subnav		ul			a(href="/treasury")				li					mtl.						treasuries			a(href="/incomesSpending")				li					mtl.						incomes_spending			a(href="/treasuryTransfer")				li					mtl.						treasury_transfer	#addBtn		a(href="/addIncomeSpendingForm")			img(src="/images/butAdd.png")			block content	h1		mtl.			incomes_spending	table.incomeSpendingTable		thead			tr				th					mtl.						description				th.odd					mtl.						date				th					mtl.						treasury				th.odd					mtl.						amount		tbody			each incomesSpending, i in incomesSpendings				tr					td.column-0.						#{incomesSpending.description}					td(class="column-1 odd").						#{incomesSpending.date}					td(class="column-2 treasury").						#{incomesSpending.treasury}					td(class="column-3 odd").						#{incomesSpending.amount}