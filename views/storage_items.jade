extends layoutblock subnav	ul		li <a href="/storage_items">Storage item</a>block content	h1=title	table.usertable		thead			tr				th.					Title				th.					Package				th.										Units				th.					Sell price				th.					Deleted		tbody			each item, i in storage_items				tr					td.						#{item.title}					td.						#{item.package}					td.						#{item.unit}					td.						#{item.sell_price}					td.						#{item.deleted}							p.		<br>		<a href="/storage">Go Back</a>			