extend layoutblock scripts	script.		$(function() {			$( "#datepicker" ).datepicker();		});	block subnav	#subnavBtn		mtl.			treasury_transfer	.subnav		ul			a(href="/treasury")				li					mtl.						treasuries			a(href="/incomesSpending")				li					mtl.						incomes_spending			a(href="/treasuryTransfer")				li					mtl.						treasury_transferblock content	h1		mtl.			treasury_transfer	form.confirmTreasuryTransfer(name="confirmTreasuryTransfer",method="post",action="/confirmTreasuryTransfer", onsubmit="return confirm('Are you sure you want to submit?')")			.formSelectTitle				mtl.					transfer_from			select(type="text", placeholder="treasury_from", name="treasuryFrom", id="treasuryFrom")				each treasury, i in treasuries					option(value="#{treasury._id}") #{treasury.title}					.formSelectTitle				mtl.					transfer_to			select(type="text", placeholder="treasury_to", name="treasuryTo", id="treasuryTo")				each treasury, i in treasuries					option(value="#{treasury._id}") #{treasury.title}			input(type="text", placeholder="amount", name="amount", required)			input(type="text", placeholder="date", name="date", id="datepicker", required)			button#btnSubmit(type="submit") submit